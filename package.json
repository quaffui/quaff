{
  "name": "@quaffui/quaff",
  "version": "0.1.0-prealpha22",
  "scripts": {
    "dev": "vite dev",
    "open": "vite dev --open",
    "build": "vite build && bun run package && bun run build:css",
    "build:css": "vite build --config vite.config.scss.ts",
    "build:docs": "bun run build:css && svelte-kit sync && vite build",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "bun run build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin prettier-plugin-svelte . --check . && eslint .",
    "format": "prettier --plugin prettier-plugin-svelte . --write . && eslint . --fix",
    "docgen-props": "bun scripts/docgenProps.ts",
    "colorgen": "bun scripts/colorgen.ts",
    "docgen-snippets": "bun scripts/docgenSnippets.ts",
    "write-version": "bun scripts/writeVersion.ts",
    "gen:plugins": "tsc --outDir plugins/dist plugins/index.ts --declaration --target esnext --module esnext --moduleResolution bundler"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./css/*": "./dist/css/*"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "shiki": "^3.4.2"
  },
  "devDependencies": {
    "@fontsource/roboto": "^5.2.8",
    "@sveltejs/adapter-static": "^3.0.9",
    "@sveltejs/kit": "^2.42.2",
    "@sveltejs/package": "^2.5.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@types/prettier": "^3.0.0",
    "@types/prismjs": "^1.26.5",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "bun-types": "^1.2.22",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-svelte": "^3.12.3",
    "eslint-plugin-unicorn": "^61.0.2",
    "estree-walker": "^3.0.3",
    "globals": "^16.4.0",
    "magic-string": "^0.30.19",
    "material-symbols": "^0.35.2",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "publint": "^0.3.13",
    "sass": "^1.92.1",
    "svelte": "^5.39.2",
    "svelte-check": "^4.3.1",
    "svelte-eslint-parser": "^1.3.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.6",
    "vitest": "^3.2.4"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "@material/material-color-utilities": "^0.3.0"
  },
  "license": "MIT"
}
