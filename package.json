{
  "name": "@quaffui/quaff",
  "version": "0.1.0-prealpha16",
  "scripts": {
    "dev": "vite dev",
    "open": "vite dev --open",
    "build": "vite build && npm run package && npm run build:css",
    "build:css": "vite build --config vite.config.scss.ts",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin prettier-plugin-svelte . --check . && eslint .",
    "format": "prettier --plugin prettier-plugin-svelte . --write .",
    "docgen": "NODE_OPTIONS='--loader ts-node/esm/transpile-only --no-warnings' node scripts/docgenProps.ts",
    "write-version": "NODE_OPTIONS='--loader ts-node/esm/transpile-only --no-warnings' node scripts/writeVersion.ts",
    "snippet": "ts-node-esm docgen/snippets/parseSnippets.ts"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./css/*": "./dist/css/*"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@fontsource/roboto": "^5.0.8",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.24.1",
    "@sveltejs/package": "^2.2.2",
    "@types/node": "^20.6.0",
    "@types/prettier": "^2.7.3",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.33.1",
    "eslint-plugin-unicorn": "^48.0.1",
    "material-symbols": "^0.12.0",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "publint": "^0.2.2",
    "sass": "^1.66.1",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.4"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "highlight.js": "^11.8.0",
    "material-dynamic-colors": "^1.0.1",
    "svelte-highlight": "^7.3.0"
  }
}
